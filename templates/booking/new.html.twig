{% extends 'base.html.twig' %}


{% block title %}New Booking{% endblock %}

{% block body %}

    <h1>Добави нов Товар</h1>


    {{ form_start(form) }}
    {{ form_row(form.loadingDate) }}
    {{ form_row(form.loadingZip) }}
    {{ form_row(form.loadingCountry) }}
    {{ form_row(form.loadingTown) }}.
    {{ form_row(form.loadingStreet) }}
    {{ form_row(form.unloadingDate) }}
    {{ form_row(form.unloadingZip) }}
    {{ form_row(form.unloadingCounty) }}
    {{ form_row(form.unloadingTown) }}.
    {{ form_row(form.unloadingStreet) }}
    {{ form_row(form.distance) }}
    {{ form_row(form.cargoWeight) }}
    {{ form_row(form.cargoHeight) }}
    {{ form_row(form.cargoLength) }}
    {{ form_row(form.price) }}
    {{ form_row(form._token) }}
    <button class="btn btn-info">{{ button_label|default('Запази') }}</button>




    <a href="{{ path('market_dash') }}">Назад</a>

{% endblock %}
